generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  active      String
  avatar      String
  createdAt   DateTime? @db.Date
  name        String
  password    String
  permissions String[]
  phone       String @unique
  updatedAt   DateTime? @db.Date
}

